<h3>Liste des publicites</h3>
<table border="2">
    <th>Nom publicite</th>
    <th>Contenu publicite</th>
    <th>Date publicite</th>
    <th>Lien</th>
    <th>Pays</th>
    <th>Region</th>
    <th>Adresse</th>
    <th>Nom catÃ©gorie</th>
    <th>image</th>
    <th>Supprimer</th>
    <th>mise</th>
    <th>profiter</th>
    {% for pub in pubs %}
    <tr>
        <td>{{ pub.pub.nompublicite }}</td>
        <td>{{ pub.pub.contenupublicte }}</td>
        <td>{{ pub.pub.datepublicite | date('y-m-d') }}</td>
        <td>{{ pub.pub.lien }}</td>
        <td>{{ pub.pub.pays }}</td>
        <td>{{ pub.pub.region }}</td>
        <td>{{ pub.pub.adresse }}</td>
        <td>{{ pub.pub.categorie.nomcategorie }}</td>
        <td> <a href="">
                <img src="{{ asset('images/') }}{{ pub.pub.nomimage }}" style="width: 300px;height: 150px">
            </a></td>
        <td><a href="{{ path('deletepublicite',{'idpublicite':pub.pub.idpublicite}) }}">Supprimer</a></td>
        <td><a href="{{ path('updatepublicite',{'idpublicite':pub.pub.idpublicite}) }}">mise</a></td>
        <td><a href="{{ path('profiterpublicite',{'idpublicite':pub.pub.idpublicite}) }}">profiter</a></td>
    </tr>
    {% endfor %}
</table>