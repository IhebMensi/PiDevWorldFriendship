<h3>Liste des lieux</h3>
<table border="2">
    <th>Pays</th>
    <th>Region</th>
    <th>Adresse</th>
    <th>Code postal</th>
    <th>Supprimer</th>
    <th>mise</th>
    {% for lieu in lieux %}
        <tr>
            <td>{{ lieu.pays }}</td>
            <td>{{ lieu.region }}</td>
            <td>{{ lieu.adresse }}</td>
            <td>{{ lieu.codepostal }}</td>
            <td><a href="{{ path('deletelieu',{'idlieu':lieu.idlieu}) }}">Supprimer</a></td>
            <td><a href="{{ path('updatelieu',{'idlieu':lieu.idlieu}) }}">mise</a></td>

        </tr>
    {% endfor %}
</table>